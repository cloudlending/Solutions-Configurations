{
     "attributes": {
          "type": "clcommon__NavigationStructure__c"
     },
     "Name": "portal_editor_branding_panel",
     "clcommon__Allow_Guest_User_Access__c": false,
     "clcommon__Description__c": null,
     "clcommon__Is_Editor_JSON__c": true,
     "clcommon__Is_Template__c": false,
     "clcommon__Related_Configuration_Mapping__c": null,
     "clcommon__Skip_Record_Access_For_Internal_Users__c": false,
     "clcommon__Unique_Name__c": "portal_editor_branding_panel",
     "clcommon__data__c": {
          "start": "BrandingPage",
          "stages": {
               "BrandingPage": {
                    "layout": {
                         "name": "BrandingPanelTabs",
                         "children": [
                              {
                                   "name": "TextColorTab",
                                   "children": [
                                        {
                                             "name": "TextColorQueueView"
                                        },
                                        {
                                             "name": "BrandingMui",
                                             "children": [
                                                  {
                                                       "name": "ColorPaletteMui"
                                                  },
                                                  {
                                                       "name": "TypographyMui"
                                                  },
                                                  {
                                                       "name": "BackgroundBorderMui"
                                                  }
                                             ]
                                        },
                                        {
                                             "name": "BrandingActionPanelMui"
                                        },
                                        {
                                             "name": "ColorPalettePopup"
                                        },
                                        {
                                             "name": "FontFamilyPopup"
                                        }
                                   ]
                              },
                              {
                                   "name": "ContentTab",
                                   "children": [
                                        {
                                             "name": "ImageQueueView"
                                        }
                                   ]
                              }
                         ]
                    }
               }
          },
          "actors": {
               "BrandingPanelTabs": {
                    "category": "container",
                    "type": "tabs",
                    "appearance": {
                         "titles": [
                              "<span>Text and Color</span>",
                              "<span>Content</span>"
                         ],
                         "current": 0
                    },
                    "generalHandlers": {
                         "switch": {
                              "navigation": "portal_editor_branding_panel"
                         }
                    }
               },
               "ImageQueueView": {
                    "category": "component",
                    "type": "queue-view",
                    "property": {
                         "structureName": "portal_editor_branding_image_queue_view"
                    },
                    "appearance": {
                         "expansionBottom": "normal",
                         "fieldColumns": 2,
                         "noLabelFields": [
                              "Id"
                         ],
                         "spanFields": [
                              "Id",
                              "Name"
                         ],
                         "sortableFields": [
                              "Name",
                              "LastModifiedDate"
                         ],
                         "sortedField": {
                              "name": "LastModifiedDate",
                              "ascending": false
                         },
                         "sortText": {
                              "sortBy": "Sort By",
                              "aToZ": "(A to Z)",
                              "zToA": "(Z to A)",
                              "mostRecentFirst": "(Most Recent First)",
                              "mostRecentLast": "(Most Recent Last)"
                         },
                         "draggableFields": {
                              "Id": {
                                   "type": "portalEditorImage"
                              }
                         }
                    },
                    "componentActions": [
                         {
                              "name": "upload",
                              "actionType": "upload",
                              "property": {
                                   "uploadType": "static-resource"
                              },
                              "appearance": {
                                   "label": "Upload",
                                   "style": "primary",
                                   "position": "bottom-right"
                              },
                              "successHandler": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$updateStaticResourceTimestamp;",
                                             "value": []
                                        }
                                   ],
                                   "updates": [
                                        {
                                             "name": "$nav.reloads.add;",
                                             "value": "ImageQueueView"
                                        }
                                   ]
                              }
                         },
                         {
                              "name": "close",
                              "actionType": "normal",
                              "appearance": {
                                   "label": "<span class=\"fa fa-times\"/>",
                                   "style": "secondary"
                              },
                              "handler": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$closePanel;",
                                             "value": [
                                                  "editBranding"
                                             ]
                                        }
                                   ]
                              }
                         }
                    ],
                    "recordActions": [
                         {
                              "name": "replace",
                              "actionType": "upload",
                              "property": {
                                   "uploadType": "static-resource",
                                   "staticResourceName": "$this.attr(Name);"
                              },
                              "appearance": {
                                   "label": "<span class='fa fa-pencil'></span>",
                                   "position": "top-right"
                              },
                              "successHandler": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$updateStaticResourceTimestamp;",
                                             "value": []
                                        }
                                   ],
                                   "updates": [
                                        {
                                             "name": "$nav.reloads.add;",
                                             "value": "ImageQueueView"
                                        }
                                   ]
                              }
                         }
                    ]
               },
               "TextColorQueueView": {
                    "category": "component",
                    "type": "queue-view",
                    "property": {
                         "structureName": "portal_editor_branding_text_color_queue_view",
                         "dataObject": "$editModeBrandingTextColor;"
                    },
                    "appearance": {
                         "expansionBottom": "normal",
                         "editable": true,
                         "render": "$state(StageBarData).attr(styleTag); == \"mint\"",
                         "renderFields": {
                              "categoryName": false,
                              "attributeId": false,
                              "overriding": false,
                              "textColor": "$this.attr(textColor); != undefined and $this.attr(textColor); != null",
                              "backgroundColor": "$this.attr(backgroundColor); != undefined and $this.attr(backgroundColor); != null",
                              "borderPosition": "$this.attr(borderPosition); != undefined and $this.attr(borderPosition); != null and $this.attr(borderColor);[1] == \"#\"",
                              "borderColor": "$this.attr(borderColor); != undefined and $this.attr(borderColor); != null",
                              "fontFamily": "$this.attr(fontFamily); != undefined and $this.attr(fontFamily); != null",
                              "fontSize": "$this.attr(fontFamily); != undefined and $this.attr(fontFamily); != null",
                              "isBold": "$this.attr(fontFamily); != undefined and $this.attr(fontFamily); != null",
                              "isItalic": "$this.attr(fontFamily); != undefined and $this.attr(fontFamily); != null"
                         },
                         "editableFields": {
                              "attributeName": false
                         },
                         "renderSections": {
                              "backgroundBorders": "$this.attr(borderTopWidth); != undefined or $this.attr(borderRightWidth); != undefined or $this.attr(borderBottomWidth); != undefined or $this.attr(borderLeftWidth); != undefined"
                         },
                         "noLabelFields": [
                              "attributeName",
                              "fontFamily",
                              "fontSize",
                              "isBold",
                              "isItalic",
                              "borderPosition"
                         ],
                         "groupBy": {
                              "field": "categoryName",
                              "title": "$this.attr(categoryName);",
                              "collapsed": true,
                              "collapsibleIcons": {
                                   "expanded": "<span class=\"fa fa-chevron-down\"/>",
                                   "collapsed": "<span class=\"fa fa-chevron-right\"/>"
                              }
                         },
                         "lifeCycle": {
                              "unmount": {
                                   "interaction": [
                                        "groupCollapseStatus"
                                   ]
                              }
                         },
                         "fieldStyle": {
                              "type": "side-by-side",
                              "labelWidth": "70px",
                              "valueWidth": "calc(100% - 70px)"
                         }
                    },
                    "componentActions": [
                         {
                              "name": "apply",
                              "actionType": "normal",
                              "appearance": {
                                   "label": "Apply",
                                   "style": "primary",
                                   "position": "bottom-right"
                              },
                              "handler": {
                                   "navigation": "portal_editor_branding_panel",
                                   "updates": [
                                        {
                                             "name": "$nav.reloads.add;",
                                             "value": "TextColorQueueView"
                                        }
                                   ],
                                   "executions": [
                                        {
                                             "name": "$applyBrandingTextColor;",
                                             "value": [
                                                  "TextColorQueueView"
                                             ]
                                        }
                                   ]
                              }
                         },
                         {
                              "name": "close",
                              "actionType": "normal",
                              "appearance": {
                                   "label": "<span class=\"fa fa-times\"/>",
                                   "style": "secondary"
                              },
                              "handler": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$closePanel;",
                                             "value": [
                                                  "editBranding"
                                             ]
                                        }
                                   ]
                              }
                         }
                    ]
               },
               "BrandingActionPanelMui": {
                    "category": "component",
                    "type": "base-panel",
                    "appearance": {
                         "render": "$state(StageBarData).attr(styleTag); == \"mui\"",
                         "expansionBottom": "normal"
                    },
                    "componentActions": [
                         {
                              "name": "apply",
                              "actionType": "normal",
                              "appearance": {
                                   "label": "Apply",
                                   "style": "primary",
                                   "position": "bottom-right"
                              },
                              "handler": {
                                   "navigation": "portal_editor_branding_panel",
                                   "updates": [
                                        {
                                             "name": "$nav.reloads.add;",
                                             "value": [
                                                  "ColorPaletteMui",
                                                  "TypographyMui",
                                                  "BackgroundBorderMui"
                                             ]
                                        }
                                   ],
                                   "executions": [
                                        {
                                             "name": "$applyBrandingMui;",
                                             "value": [
                                                  "ColorPaletteMui",
                                                  "TypographyMui",
                                                  "BackgroundBorderMui"
                                             ]
                                        }
                                   ]
                              }
                         },
                         {
                              "name": "close",
                              "actionType": "normal",
                              "appearance": {
                                   "label": "<span class=\"fa fa-times\"/>",
                                   "style": "secondary"
                              },
                              "handler": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$closePanel;",
                                             "value": [
                                                  "editBranding"
                                             ]
                                        }
                                   ]
                              }
                         }
                    ]
               },
               "ColorPaletteMui": {
                    "category": "component",
                    "type": "base-panel",
                    "property": {
                         "structureName": "portal_editor_branding_color_palette_mui",
                         "dataObject": "$editModeBrandingColorPaletteMui;"
                    },
                    "appearance": {
                         "editable": true,
                         "render": "$state(StageBarData).attr(styleTag); == \"mui\"",
                         "expansionTop": "normal",
                         "fieldColumns": 4,
                         "title": "Color Palette",
                         "showSectionTitle": true,
                         "editableFields": {
                              "primaryLight": false,
                              "primaryDark": false,
                              "secondaryLight": false,
                              "secondaryDark": false,
                              "errorLight": false,
                              "errorDark": false
                         }
                    },
                    "generalHandlers": {
                         "fieldChange": {
                              "primaryMain": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$updateBrandingColorPaletteMui;",
                                             "value": [
                                                  "primary"
                                             ]
                                        }
                                   ]
                              },
                              "secondaryMain": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$updateBrandingColorPaletteMui;",
                                             "value": [
                                                  "secondary"
                                             ]
                                        }
                                   ]
                              },
                              "errorMain": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$updateBrandingColorPaletteMui;",
                                             "value": [
                                                  "error"
                                             ]
                                        }
                                   ]
                              }
                         }
                    }
               },
               "TypographyMui": {
                    "category": "component",
                    "type": "base-panel",
                    "property": {
                         "structureName": "portal_editor_branding_typography_mui",
                         "dataObject": "$editModeBrandingTypographyMui;"
                    },
                    "appearance": {
                         "editable": true,
                         "render": "$state(StageBarData).attr(styleTag); == \"mui\"",
                         "expansionTop": "normal",
                         "title": "Typography",
                         "fieldStyle": {
                              "type": "side-by-side",
                              "labelWidth": "40%",
                              "valueWidth": "60%"
                         },
                         "editableFields": {
                              "fontBody": false,
                              "fontBodySmall": false,
                              "fontH1": false,
                              "fontH2": false,
                              "fontH3": false,
                              "fontButton": false
                         }
                    },
                    "generalHandlers": {
                         "fieldChange": {
                              "fontSize": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$updateBrandingTypographyMui;",
                                             "value": [
                                                  "fontSize"
                                             ]
                                        }
                                   ]
                              },
                              "fontWeight": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$updateBrandingTypographyMui;",
                                             "value": [
                                                  "fontWeight"
                                             ]
                                        }
                                   ]
                              }
                         }
                    }
               },
               "BackgroundBorderMui": {
                    "category": "component",
                    "type": "base-panel",
                    "property": {
                         "structureName": "portal_editor_branding_background_border_mui",
                         "dataObject": "$editModeBrandingBackgroundBorderMui;"
                    },
                    "appearance": {
                         "editable": true,
                         "render": "$state(StageBarData).attr(styleTag); == \"mui\"",
                         "expansionTop": "normal",
                         "fieldColumns": 2,
                         "title": "Background and Border",
                         "showSectionTitle": true,
                         "spanFields": [
                              "borderRadius"
                         ],
                         "noLabelFields": [
                              "divisionBorderRadius",
                              "divisionShadow",
                              "componentBorderRadius",
                              "componentShadow",
                              "contentBorderRadius",
                              "contentShadow",
                              "containerBorderRadius",
                              "containerShadow"
                         ]
                    }
               },
               "ColorPalettePopup": {
                    "mode": "popup",
                    "category": "component",
                    "type": "base-panel",
                    "property": {
                         "structureName": "portal_editor_branding_color_palette_popup",
                         "dataObject": "$editModeBrandingColorPalette;"
                    },
                    "appearance": {
                         "editable": true,
                         "transient": true,
                         "noLabelFields": [
                              "currentColor"
                         ],
                         "popup": {
                              "direction": "bottom",
                              "anchor": "cursor",
                              "width": 234,
                              "height": 308,
                              "margin": 20,
                              "borderColor": "#767676",
                              "backgroundColor": "#ffffff"
                         }
                    },
                    "generalHandlers": {
                         "fieldChange": {
                              "currentColor": {
                                   "navigation": "portal_editor_branding_panel",
                                   "executions": [
                                        {
                                             "name": "$updateBrandingColor;",
                                             "value": [
                                                  "ColorPalettePopup"
                                             ]
                                        }
                                   ]
                              }
                         }
                    }
               },
               "FontFamilyPopup": {
                    "mode": "popup",
                    "category": "component",
                    "type": "queue-view",
                    "property": {
                         "structureName": "portal_editor_branding_font_family_popup",
                         "dataObject": "$editModeBrandingFontFamily;"
                    },
                    "appearance": {
                         "transient": true,
                         "groupBy": {
                              "field": "categoryName",
                              "title": "$this.attr(categoryName);"
                         },
                         "renderFields": {
                              "categoryName": "false",
                              "fontFamilyName": "false"
                         },
                         "noLabelFields": [
                              "fontFamilyDisplay"
                         ],
                         "highlight": true,
                         "autoHighlight": {
                              "fontFamilyName": "$interaction.attr(popupDynamic).attr(data);"
                         },
                         "popup": {
                              "direction": "bottom",
                              "anchor": "cursor",
                              "width": 250,
                              "height": 400,
                              "margin": 20,
                              "borderColor": "#767676",
                              "backgroundColor": "#ffffff",
                              "overflowY": "auto"
                         }
                    },
                    "generalHandlers": {
                         "clickRecord": {
                              "navigation": "portal_editor_branding_panel",
                              "executions": [
                                   {
                                        "name": "$updateBrandingFontFamily;",
                                        "value": [
                                             "FontFamilyPopup"
                                        ]
                                   }
                              ]
                         }
                    }
               }
          },
          "settings": {
               "locale": "en-us",
               "currency": "USD",
               "minimumFractionDigits": 2,
               "uploadText": {
                    "completed": "Completed",
                    "cancelInProgress": "Cancel",
                    "removeError": "<span class=\"fa fa-times\"></span>"
               }
          },
          "messages": {
               "loadingAnimationText": "<span><span class=\"fa fa-spinner\"></span>Loading...</span>",
               "savingAnimationText": "<span><span class=\"fa fa-spinner\"></span>Saving...</span>",
               "submitAnimationText": "<span><span class=\"fa fa-spinner\"></span>Sending...</span>",
               "loadingFailureText": "<span><span class=\"fa fa-exclamation-triangle\"></span>Fail to load</span>",
               "saveSuccessText": "<span><span class=\"fa fa-check\"></span>Succeed</span>",
               "saveFailureText": "<span><span class=\"fa fa-exclamation-triangle\"></span>Fail to save</span>",
               "submitSuccessText": "<span><span class=\"fa fa-check\"></span>Succeed</span>",
               "submitFailureText": "<span><span class=\"fa fa-exclamation-triangle\"></span>Fail to submit</span>",
               "submitExpirationText": "<span><span class=\"fa fa-exclamation-triangle\"></span>Fail to submit</span>",
               "uploadFailureText": "<span><span class=\"fa fa-exclamation-triangle\"></span>Fail to upload</span>",
               "uploadFileTooLargeError": "<span><span class=\"fa fa-exclamation-triangle\"></span>File too large</span>"
          }
     },
