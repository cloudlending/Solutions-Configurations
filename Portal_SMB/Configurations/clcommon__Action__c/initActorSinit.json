                    }
               ],
               "actions": [
                    {
                         "condition": "$result.attr(portalStage); == \"BusinessEntity\" or $result.attr(portalStage); == \"BusinessInformation\" or $result.attr(portalStage); == \"BusinessAnnualRevenue\" or $result.attr(portalStage); == \"BusinessFinancials\"",
