                         "condition": "$result.attr(portalStage); == \"ReviewAndSubmit\" or $result.attr(portalStage); == \"ApplicationSubmitted\"",
                         "name": "goToReview"
                    },
                    {
                         "condition": "$result.attr(portalStage); == \"CollateralDetails\"",
                         "name": "goToCollateral"
